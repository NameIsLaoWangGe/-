  "code": "import { LwgEvent, LwgTimer, LwgTools } from \"../../Lwg/Lwg\";\r\nimport { _Game } from \"../_GameData\";\r\nimport { _Res } from \"../_Res\";\r\nexport class _CreateBullet {\r\n    static checkHero(bullet) {\r\n        LwgTimer._frameLoop(1, bullet, () => {\r\n            const bool = LwgTools._Node.leaveStage(bullet, () => {\r\n                bullet.removeSelf();\r\n            });\r\n            if (!bool && bullet.name === _CreateBullet._bulletType.single) {\r\n                LwgEvent._notify(_Game._Event.checkEnemyBullet, [bullet, 1]);\r\n            }\r\n        });\r\n    }\r\n    static createBase(enemy, type) {\r\n        let prefab;\r\n        switch (type) {\r\n            case this._bulletType.single:\r\n                prefab = _Res.$prefab2D.EnemyBullet.prefab2D;\r\n                break;\r\n            case this._bulletType.two:\r\n                prefab = _Res.$prefab2D.EnemyBulletTow.prefab2D;\r\n                break;\r\n            case this._bulletType.three:\r\n                prefab = _Res.$prefab2D.EnemyBulletThree.prefab2D;\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n        const bullet = LwgTools._Node.createPrefab(prefab, this.Parent, [enemy._lwg.gPoint.x, enemy._lwg.gPoint.y]);\r\n        bullet.name = type;\r\n        this.checkHero(bullet);\r\n        return bullet;\r\n    }\r\n    static _single(enemy) {\r\n        const bullet = this.createBase(enemy, this._bulletType.single);\r\n        return bullet;\r\n    }\r\n    static _two(enemy) {\r\n        const bullet = this.createBase(enemy, this._bulletType.two);\r\n        for (let index = 0; index < bullet.numChildren; index++) {\r\n            const element = bullet.getChildAt(index);\r\n            this.checkHero(element);\r\n            element.name = this._bulletType.single;\r\n        }\r\n        return bullet;\r\n    }\r\n    static _three(enemy) {\r\n        const bullet = this.createBase(enemy, this._bulletType.three);\r\n        bullet.name = this._bulletType.three;\r\n        for (let index = 0; index < bullet.numChildren; index++) {\r\n            const element = bullet.getChildAt(index);\r\n            this.checkHero(element);\r\n            element.name = this._bulletType.single;\r\n        }\r\n        return bullet;\r\n    }\r\n}\r\n_CreateBullet._bulletType = {\r\n    single: '_single',\r\n    two: '_two',\r\n    three: '_three',\r\n};\r\n",
